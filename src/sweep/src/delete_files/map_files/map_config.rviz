Panels:
  Class: rviz/Displays
  Help Height: 0
  Name: Displays
  Property Tree Widget:
    Expanded:
    - 'true'
    - /Static Map
    - /AMCL
    - /LaserScan
    Splitter Ratio: '0.5'
Visualization Manager:
  Class: rviz/VisualizationManager
  Displays:
  - Alpha: 0.7
    Class: rviz/Map
    Enabled: true
    Name: Static Map
    Topic: /map
    Value: true
  - Class: rviz/Group
    Displays:
    - Class: rviz/PoseArray
      Enabled: true
      Name: Particle Cloud
      Topic: /particlecloud
    - Class: rviz/Pose
      Enabled: true
      Name: Robot Pose
      Topic: /amcl_pose
    Enabled: true
    Name: AMCL
  - Class: rviz/LaserScan
    Enabled: true
    Name: LaserScan
    Size (m): 0.05
    Topic: /scan
  - Class: rviz/RobotModel
    Enabled: true
    Name: Robot Model
  Global Options:
    Fixed Frame: map
    Frame Rate: 30
  Tools:
  - Class: rviz/MoveCamera
  - Class: rviz/Interact
  - Class: rviz/Select
  - Class: rviz/SetInitialPose
    Topic: /initialpose
  - Class: rviz/SetGoal
    Topic: /move_base_simple/goal
  Views:
    Current:
      Class: rviz/Orbit
      Distance: 10
      Enable Stereo Rendering: false
      Eye:
        x: 0
        y: 0
        z: 10
      Focal Point:
        x: 0
        y: 0
        z: 0
      Near Clip Distance: 0.01
      Pitch: 1.5708
      Target Frame: map
      Value: TopDownOrtho
      Yaw: 0
